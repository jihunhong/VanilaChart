generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Charts {
  id        Int      @id @default(autoincrement())
  rank      Int
  title     String
  artist    String
  album     String
  site      String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model Music {
  id         Int       @id @default(autoincrement())
  title      String
  artist     String    @unique
  videoId    String    @unique
  album      String
  created_at DateTime?
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @default(now())
}

model Users {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  nickname  String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
